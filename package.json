{
  "name": "navch",
  "version": "1.0.0",
  "description": "A package of common JavaScript sugars",
  "main": "lib/index.js",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.1.18",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babel-register": "^6.3.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.1",
    "eslint-plugin-react": "^3.11.2",
    "gulp-babel": "^6.1.1",
    "gulp-mocha": "^2.2.0",
    "gulp-notify": "^2.2.0",
    "gulp-watch": "^4.3.5",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4",
    "should": "^7.1.1",
    "webpack": "^1.12.9",
    "yargs": "^3.31.0"
  },
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "test": "mocha --compilers js:babel/register test/*.js",
    "test:watch": "npm test -- --watch",
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:watch": "npm run build:lib -- --watch",
    "build:lib": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/navch.js --config webpack.config.dev.js",
    "build:umd:min": "webpack src/index.js dist/navch.min.js --config webpack.config.prod.js",
    "preversion": "npm run clean && npm run check",
    "version": "npm run build",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/navch/navch-js.git"
  },
  "author": "Zhenwen Cai <frederickcai@gmail.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/navch/navch-js#readme"
}
